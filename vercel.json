{\n  \"version\": 2,\n  \"builds\": [\n    {\n      \"src\": \"frontend/package.json\",\n      \"use\": \"@vercel/static-build\",\n      \"config\": {\n        \"buildCommand\": \"cd frontend && npm install && npm run build\",\n        \"distDir\": \"frontend/dist\"\n      }\n    }\n  ],\n  \"routes\": [\n    {\n      \"src\": \"/(.*)\",\n      \"dest\": \"/frontend/dist/$1\"\n    }\n  ],\n  \"headers\": [\n    {\n      \"source\": \"/(.*)\",\n      \"headers\": [\n        {\n          \"key\": \"X-Content-Type-Options\",\n          \"value\": \"nosniff\"\n        },\n        {\n          \"key\": \"X-Frame-Options\",\n          \"value\": \"DENY\"\n        },\n        {\n          \"key\": \"X-XSS-Protection\",\n          \"value\": \"1; mode=block\"\n        }\n      ]\n    }\n  ]\n}